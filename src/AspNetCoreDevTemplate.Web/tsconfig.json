{
    "compilerOptions": {
        "noImplicitAny": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "outDir": "./wwwroot/ts/dist",
        "strictNullChecks": true,
        "target": "ES5",
        "typeRoots": [
            "./node_modules/@types",
            "./wwwroot/ts/src/@types"
        ]
    },
    "include": [
        "./wwwroot/ts/src/**/*"
    ]
}